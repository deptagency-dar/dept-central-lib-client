{
  "name": "dept-central-lib-client",
  "version": "1.26.1",
  "description": "A lightweight typeScript react component library",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=11.0.0"
  },
  "scripts": {
    "build": "npm run clean && webpack",
    "build:prod": "npm run clean && webpack --mode production",
    "clean": "rimraf dist",
    "dev": "webpack --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "commitlint": "commitlint --edit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deptagency-dar/dept-central-lib-client"
  },
  "keywords": [
    "react",
    "typescript",
    "components",
    "lib",
    "dept-ui",
    "dept-central-ui",
    "dept-central-lib-client"
  ],
  "peerDependencies": {
    "react": ">=16.11.0",
    "react-dom": ">=16.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-styling": "^1.3.6",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.6",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/blocks": "^8.6.12",
    "@storybook/manager-api": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-webpack5": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/theming": "^8.6.12",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "autoprefixer": "^10.4.14",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.12.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.27",
    "postcss-loader": "^8.1.0",
    "prettier": "^3.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^5.0.1",
    "semantic-release": "^23.0.2",
    "storybook": "^8.6.12",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@heroicons/react": "^2.0.18",
    "classnames": "^2.5.1",
    "motion": "^12.5.0",
    "react-laag": "^2.0.5",
    "tailwindcss": "^3.3.3"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ]
}
